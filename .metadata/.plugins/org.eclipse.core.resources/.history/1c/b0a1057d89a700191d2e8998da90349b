package maccess;
public class DWrapperClass1 {
	public static void main(String[] args) {
		//Boxing process
		Integer ob1 = new Integer(12);
		Integer ob2 = new Integer("13");
		System.out.println("--Dis from objects");
		System.out.println("ob1:" +ob1.toString());
		System.out.println("ob2:" +ob2.toString());
		System.out.println("---Arithmetic operations----");
		System.out.println("sum:"+(ob1+ob2));
		System.out.println("sub:"+(ob1-ob2));
		System.out.println("mul:"+(ob1*ob2));
		System.out.println("div:"+(float)ob1/ob2);
		System.out.println("ModDiv:"+(ob1%ob2));
		
	}

}
